extends nav

block append content
    .hero-wrap
      .overlay
      .circle-bg
      .circle-bg-2
      .container-fluid
        .row.no-gutters.d-flex.slider-text.align-items-center.justify-content-center(data-scrollax-parent='true')
          .col-md-6.ftco-animate.text-center(data-scrollax=" properties: { translateY: '70%' }")
            p.breadcrumbs(data-scrollax="properties: { translateY: '30%', opacity: 1.6 }")
              span.mr-2
                a(href='/') Home
              span Domain
            h1.mb-3.bread(data-scrollax="properties: { translateY: '30%', opacity: 1.6 }") Courses Offered
    section.ftco-section.bg-light
      .container
        .row.justify-content-center.mb-5
          .col-md-7.text-center.heading-section.ftco-animate
            span#services.subheading Get domain
            h2.mb-4 Get A Domain Name
            p With FREE Email, DNS, Theft Protection, and other features
        .row.justify-content-center
          .col-md-10.ftco-animate
            form.domain-form(action='#')
              .form-group.d-md-flex
                input.form-control.px-4(type='text' placeholder='Enter your domain name...')
                input.search-domain.btn.btn-primary.px-5(type='submit' value='Search Domain')
            p.domain-price.text-center
              span
                small .com
                |  9.75
              span
                small .net
                |  9.90
              span
                small .biz
                |  $8.95
              span
                small .me
                |  $7.95
    section.ftco-section
      .container
        .row.justify-content-center.mb-5
          .col-md-7.text-center.heading-section.ftco-animate
            span#services.subheading Pricing Plans
            h2.mb-4 Domain Pricing
        .row
          .col-md-12.ftco-animate
            .table-responsive
              table.table
                thead.thead-primary
                  tr
                    th TLD
                    th Duration
                    th Registration
                    th Renewal
                    th Transfer
                tbody
                  tr
                    td .com
                    td 1 Year
                    td $70.00
                    td $5.00
                    td $5.00
                  tr
                    td .net
                    td 1 Year
                    td $75.00
                    td $5.00
                    td $5.00
                  tr
                    td .org
                    td 1 Year
                    td $65.00
                    td $5.00
                    td $5.00
                  tr
                    td .biz
                    td 1 Year
                    td $60.00
                    td $5.00
                    td $5.00
                  tr
                    td .info
                    td 1 Year
                    td $50.00
                    td $5.00
                    td $5.00
                  tr
                    td .me
                    td 1 Year
                    td $45.00
                    td $5.00
                    td $5.00
